# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key

# Qdrant Cloud Configuration
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your-qdrant-api-key

# Google AI Configuration
GOOGLE_API_KEY=your-google-api-key
EMBEDDING_MODEL=gemini-embedding-001
EMBEDDING_DIMENSIONS=3072
LLM_MODEL=gemini-2.5-flash-preview-05-20

# Server Configuration
LOG_LEVEL=INFO

# Authentication Configuration
# JWT secret key for signing tokens (minimum 32 characters)
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=your-secret-key-minimum-32-characters-long
AUTH_ISSUER=gim-mcp
AUTH_AUDIENCE=gim-clients
ACCESS_TOKEN_TTL_HOURS=1

# Transport Configuration
# Options: stdio (default), http, dual
TRANSPORT_MODE=stdio
HTTP_HOST=0.0.0.0
HTTP_PORT=8000

# CORS / Frontend
# Set to your production frontend URL for CORS (e.g., https://your-app.vercel.app)
FRONTEND_URL=

# Rate Limiting
DEFAULT_DAILY_SEARCH_LIMIT=100

# GitHub Crawler (optional)
GITHUB_TOKEN=
